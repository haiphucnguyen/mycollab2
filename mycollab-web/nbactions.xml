<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>CUSTOM-Web Launcher</actionName>
            <displayName>Web Launcher</displayName>
            <goals>
                <goal>jetty:run</goal>
                <goal>-Djetty.port=8080</goal>
            </goals>
            <properties>
                <skipTests>true</skipTests>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <goals>
                <goal>compile</goal>
            </goals>
            <properties>
                <netbeans.deploy.debugmode>true</netbeans.deploy.debugmode>
                <netbeans.deploy>true</netbeans.deploy>
            </properties>
        </action>
        <action>
            <actionName>run</actionName>
            <goals>
                <goal>compile</goal>
            </goals>
            <properties>
                <netbeans.deploy>true</netbeans.deploy>
            </properties>
        </action>
        <action>
            <actionName>CUSTOM-Run Jetty</actionName>
            <displayName>Run Jetty</displayName>
            <goals>
                <goal>jetty:run</goal>
                <goal>-Djetty.port=8080</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-Compile</actionName>
            <displayName>Compile</displayName>
            <goals>
                <goal>compile</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-Compile widgetset</actionName>
            <displayName>Compile widgetset</displayName>
            <goals>
                <goal>vaadin:update-widgetset</goal>
                <goal>gwt:compile</goal>
            </goals>
        </action>
    </actions>
