<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

    <context id="MySQL" targetRuntime="MyBatis3">
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
            connectionURL="jdbc:mysql://localhost:3306/mycollab" userId="root"
            password="esofthead321" />

        <javaModelGenerator targetPackage="com.esofthead.mycollab.module.file.domain"
            targetProject="src/main/java">
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="."
            targetProject="src/main/resources/sqlMap/file" />

        <javaClientGenerator targetPackage="com.esofthead.mycollab.module.file.dao"
            type="XMLMAPPER" targetProject="src/main/java">
            <property name="exampleMethodVisibility" value="public" />
            <property name="rootInterface"
                value="com.esofthead.mycollab.core.persistence.ICrudGenericDAO" />
        </javaClientGenerator>
        
        <table schema="mycollab" tableName="m_attachment"
            domainObjectName="Attachment" />
    </context>
</generatorConfiguration>
