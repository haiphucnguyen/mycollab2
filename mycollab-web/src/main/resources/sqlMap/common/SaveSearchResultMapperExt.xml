<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.esofthead.mycollab.common.dao.SaveSearchResultMapperExt">
  <select id="getListSaveSearchResult" 
  	parameterType="com.esofthead.mycollab.common.domain.criteria.SaveSearchResultCriteria" 
  	resultMap="com.esofthead.mycollab.common.dao.SaveSearchResultMapper.ResultMapWithBLOBs">
  		SELECT queryText FROM s_save_search_result 
  		WHERE type = #{type,jdbcType=VARCHAR} AND
  		queryName = #{queryName, jdbcType=VARCHAR} AND
  		sAccountId = #{sAccountId , jdbcType=INTEGER}
  </select>
</mapper>