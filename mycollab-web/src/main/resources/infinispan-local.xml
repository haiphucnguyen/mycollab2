<?xml version="1.0" encoding="UTF-8"?>
<infinispan xsi:schemaLocation="urn:infinispan:config:5.3 infinispan-config-5.3.xsd"
    xmlns="urn:infinispan:config:5.3" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <globalJmxStatistics enabled="false"
        allowDuplicateDomains="true" />
    <default>
        <jmxStatistics enabled="true" />
        <eviction maxEntries="10000" strategy="LRU" />
    </default>
</infinispan>