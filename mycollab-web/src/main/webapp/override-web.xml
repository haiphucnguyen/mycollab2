<?xml version="1.0"  encoding="ISO-8859-1"?>
<!DOCTYPE Configure PUBLIC "-//Jetty//Configure//EN" "http://www.eclipse.org/jetty/configure.dtd">

<Configure class="org.eclipse.jetty.webapp.WebAppContext">
    <Set name="contextPath">/mycollab-web</Set>
    <Set name="resourceBases">
        <Item><New class="java.lang.String"><Arg><SystemProperty name="jetty.home" default="."/>/assets/</Arg></New></Item>
        <Item><New class="java.lang.String"><Arg><SystemProperty name="jetty.home" default="."/>/VAADIN/</Arg></New></Item>
    </Set>
</Configure>