<module>

	<!-- WS Compiler: manually edited -->

	<!-- Inherit DefaultWidgetSet -->
	<inherits name="com.vaadin.DefaultWidgetSet" />

	<!-- Inherit TouchkitWidgetSet -->
	<inherits name="com.vaadin.addon.touchkit.gwt.TouchKitWidgetSet" />

	<replace-with class="com.mycollab.vaadin.touchkit.client.VMobileNavigationBar">
		<when-type-is class="com.vaadin.addon.touchkit.gwt.client.ui.VNavigationBar" />
	</replace-with>

	<replace-with class="com.mycollab.vaadin.touchkit.client.ui.TouchScrollHandlerExt">
		<when-type-is class="com.vaadin.client.ui.TouchScrollDelegate.TouchScrollHandler" />
	</replace-with>

	<!-- Widget styles in public -directory -->
	<stylesheet src="quickmenu.css"/>

	<stylesheet src="mobilecomponent.css"></stylesheet>

</module>
