<module>

	<!-- WS Compiler: manually edited -->

	<!-- Inherit DefaultWidgetSet -->
	<inherits name="com.vaadin.DefaultWidgetSet" />
	
	<!-- Inherit TouchkitWidgetSet -->
	<inherits name="com.vaadin.addon.touchkit.gwt.TouchKitWidgetSet" />
	
	<replace-with class="com.esofthead.vaadin.mobilecomponent.client.VMobileNavigationBar">
		<when-type-is class="com.vaadin.addon.touchkit.gwt.client.ui.VNavigationBar" />
	</replace-with>

	<!-- Widget styles in public -directory -->
	<stylesheet src="mobilecomponent/mobilecomponent.css"/>

</module>