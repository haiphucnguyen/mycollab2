    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
        http://www.springframework.org/schema/osgi http://www.springframework.org/schema/osgi/spring-osgi.xsd">


    <!-- Spring Bean DAO -->
    <bean class="org.mybatis.spring.mapper.MapperFactoryBean" id="accountSettingsMapper">
        <property name="sqlSessionTemplate" ref="sqlMapClient" />
        <property name="mapperInterface"
            value="com.esofthead.mycollab.module.user.dao.AccountSettingsMapper" />
    </bean>

    <bean class="org.mybatis.spring.mapper.MapperFactoryBean" id="billingAccountMapper">
