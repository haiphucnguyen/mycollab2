<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

    <context
        id="MySQL"
        targetRuntime="MyBatis3" >
        
        <jdbcConnection
            connectionURL="jdbc:mysql://localhost:3306/ip_country"
            driverClass="com.mysql.jdbc.Driver"
            password="oldtrafford"
            userId="root" />

        <javaModelGenerator
            targetPackage="com.esofthead.mycollab.usertracking.domain"
            targetProject="src/main/java" >
        </javaModelGenerator>

        <sqlMapGenerator
            targetPackage="."
            targetProject="src/main/resources/sqlMap/usertracking" />

        <javaClientGenerator
            targetPackage="com.esofthead.mycollab.usertracking.dao"
            targetProject="src/main/java"
            type="XMLMAPPER" >

            <property
                name="exampleMethodVisibility"
                value="public" />
        </javaClientGenerator>

        <table
            domainObjectName="IpCountry"
            schema="mycollab_live"
            tableName="mapper" />
    </context>

</generatorConfiguration>